# Staging Environment Configuration
# Clean Architecture .NET Project

# Application Settings
ASPNETCORE_ENVIRONMENT=Staging
TAG=staging

# Database Configuration
SQL_PASSWORD=StrongPassword123!
MSSQL_PID=Express

# Redis Configuration
REDIS_PASSWORD=RedisPassword123!

# RabbitMQ Configuration
RABBITMQ_USER=admin
RABBITMQ_PASSWORD=RabbitPassword123!

# Logging Configuration
LOG_LEVEL=Information

# Security Settings
JWT_SECRET=YourStagingJwtSecretKeyThatShouldBeAtLeast32Characters!
JWT_ISSUER=CleanArchitecture.Staging
JWT_AUDIENCE=CleanArchitecture.API.Staging
JWT_EXPIRY_MINUTES=60

# API Configuration
API_VERSION=v1
API_TITLE=Clean Architecture API - Staging
CORS_ORIGINS=http://localhost:3000,https://staging.yourdomain.com

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=30

# Performance Settings
CONNECTION_POOL_SIZE=100
REDIS_CONNECTION_POOL_SIZE=50
RABBITMQ_CONNECTION_POOL_SIZE=20

# Monitoring
ENABLE_METRICS=true
ENABLE_TRACING=true